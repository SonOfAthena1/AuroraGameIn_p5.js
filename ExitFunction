function EXIT(){
		if(mouseX<125&&mouseX>25 && (mouseY<75&&mouseY>25)){
			fill('rgb(255,0,0)')}
		else{
			noFill()}
		stroke(125)
		strokeWeight(2)
		rect(75,50,100,50)
		
		textAlign(CENTER)
		stroke('cyan')
		strokeWeight(1.5)
		fill('cyan')
		textSize(30)
		text("EXIT",75,60)
	
	if(mouseIsPressed===true && mouseX<125&&mouseX>25 && mouseY<75&&mouseY>25){
		titleScreen=0
	}
}
