class star{
	constructor(){
		this.x = random(1,699)
		this.y = random(0,699)
		this.size = random(1,4)
		this.texture = random(200,250)
		this.stardrag = random(1,3)
	}
	
	render(){
		stroke(250)
		strokeWeight(this.size)
		this.y+=starSpeed*this.size
		point(this.x,this.y)
		
		if(this.y>600){
			this.y-=700
		}
	}
}// END STAR
